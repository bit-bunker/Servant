{
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "build": "npm run build:clean && npm run build:compile",
    "build:start": "npm run build && npm run start",
    "build:compile": "swc src -d build -D",
    "build:clean": "rimraf build/",
    "watch": "concurrently \"npm run watch:compile\" \"npm run watch:start\"",
    "watch:compile": "swc src -w -d build -D",
    "watch:start": "nodemon --watch \"build/**/*\" -e js ./build/index.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/pagination": "^3.4.0",
    "@discordx/utilities": "^5.2.1",
    "@prisma/client": "4.15.0",
    "@types/node-cron": "^3.0.8",
    "discord.js": "^14.7.1",
    "discordx": "^11.7.0",
    "dotenv": "^16.0.3",
    "node-cron": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.61",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "chokidar": "^3.5.3",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "prisma": "^4.15.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
